programa {
  funcao inicio() {

    //Declaração de variaveis 
    inteiro numero1, numero2, soma

    //Entrada de dados
    escreva("Digite o primeiro numero: ")
    leia(numero1)

    escreva("digite o segundo numero: ")
    leia(numero2)

    //Calculo, processamento, logica
    soma = numero1 + numero2

    //Saida de dados
    escreva("SOMA = ", soma)


  }
}
